{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@wormhole-foundation/ntt-cli",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "prepublish": "npm run build && npm run copy-script",
    "build": "rollup -c && cp ../evm/script/DeployWormholeNtt.s.sol ../evm/script/helpers/DeployWormholeNttBase.sol dist"
  },
  "files": [
    "script/*",
    "dist",
    "./bin.cjs"
  ],
  "bin": "./bin.cjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/yargs": "^17.0.32",
    "rollup": "^4.20.0",
    "rollup-plugin-string-import": "^1.2.5",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.29.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/spl-token-metadata": "^0.1.6",
    "@solana/web3.js": "^1.98.2",
    "@wormhole-foundation/sdk": "^1.14.2",
    "@wormhole-foundation/sdk-connect": "^1.14.2",
    "@wormhole-foundation/sdk-definitions-ntt": "0.5.0",
    "@wormhole-foundation/sdk-evm": "^1.14.2",
    "@wormhole-foundation/sdk-evm-ntt": "0.5.0",
    "@wormhole-foundation/sdk-solana": "^1.14.2",
    "@wormhole-foundation/sdk-solana-ntt": "0.5.0",
    "chalk": "4.1.2",
    "ethers": "6.14.1",
    "yargs": "17.7.2"
  }
}
