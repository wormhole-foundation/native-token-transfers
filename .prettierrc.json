{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "plugins": ["prettier/plugins/babel"],
  "overrides": [
    {
      "files": ["cli/src/*.ts"],
      "options": {
        "parser": "babel-ts"
      }
    }
  ]
}
